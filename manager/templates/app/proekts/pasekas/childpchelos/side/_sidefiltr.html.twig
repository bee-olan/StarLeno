<aside class="sidebarr">

    <div class="sidebarr__header">
        <div class="logo_es"></div>
    </div>

    <div class="sideefiltr">

        {{ form_start(form) }}
        <ul class="sideefiltr__ul">
            <li class="sideefiltr__li sideefiltr__li--wid100 sideefiltr__li--flex">
                <div class="pa-ri-05">
                    <button type="submit" class="  btnn  btnn-filtr">Фильтр</button>
                </div>
                <div class="sidee__lupa">
                    <div class="lupaimg"></div>
                </div>
                <div>
                    <a class=" btnn  btnn-filtr "
                       href="{{ path('app.proekts.pasekas.pchelomatkas.spisoks') }}">Сбросить</a>
                </div>
            </li>
            <li class="child-tabl-col-pchelo">
                По названию
            </li>
            <li class="sideefiltr__li sideefiltr__li--wid100">
                <a class="sideefiltr__link ">{{ form_widget(form.text) }}</a>
            </li>
            <li class="sideefiltr__li sideefiltr__li--wid100">
                <a class="sideefiltr__link "> {{ form_widget(form.author) }}</a>
            </li>
            <li class="sideefiltr__li sideefiltr__li--wid100">
                <a class="sideefiltr__link "> {{ form_widget(form.executor) }}</a>
            </li>
            {#            <li class="side-title-pchel__li">#}
            {#              Статус Дочь - Пчело#}
            {#            </li>#}
            <li class="sideefiltr__li sideefiltr__li--wid100">
                <a class="sideefiltr__link ">{{ form_widget(form.status) }}</a>
            </li>
            {#             <li class="side-title-pchel__li">#}
            {#              Выбор спарга#}
            {#            </li>#}
            <li class="sideefiltr__li sideefiltr__li--wid100">
                <div class="fil-pchel">
                    <a class="sideefiltr__link ">{{ form_widget(form.type) }}</a>
                </div>
            </li>

            {#            <li class="side-title-pchel__li">#}
            {#              Выбор приорита#}
            {#            </li>#}
            <li class="sideefiltr__li sideefiltr__li--wid100">
                <a class="sideefiltr__link ">{{ form_widget(form.priority) }}</a>
            </li>


            {#            <li class="side-title-pchel__li">#}
            {#                Уровень вложения#}
            {#            </li>#}
            <li class="sideefiltr__li sideefiltr__li--wid100">
                <a class="sideefiltr__link ">{{ form_widget(form.urowni) }}</a>
            </li>

            {#            <li class="side-title-pchel__li">#}
            {#                Все уровни#}
            {#            </li>#}
            <li class="sideefiltr__li sideefiltr__li--wid100">
                <a class="sideefiltr__link ">{{ form_widget(form.roots) }}</a>
            </li>

        </ul>
        {{ form_end(form) }}
    </div>
    <div class="profilee__text">
        <div class="profilee__header">
            <div class="profilee__name"> Внимание!</div>
            <div class="profilee__prof ma-bo-2"> Если что-то пошло <br>не так</div>
